{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2023", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "allowJs": false,
    "jsx": "preserve",
    "incremental": true,
    "strict": true,
    "types": ["node"],
    "paths": {},
    "baseUrl": "."
